{
	"hosting": {
		"public": ".dist/weather-app",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"redirects": [
			{
				"source": "/app",
				"destination": "/",
				"type": 308
			},
			{
				"source": "/app/home",
				"destination": "/",
				"type": 308
			},
			{
				"source": "/app/forecast",
				"destination": "/",
				"type": 308
			},
			{
				"source": "/app/about-me",
				"destination": "/",
				"type": 308
			}
		]
	}
}
