		"open": "^7.0.3",
		"prettier": "^2.2.1",
		"protractor": "~7.0.0",
		"ts-jest": "^26.4.4",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"tslint-angular": "^3.0.3",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "~4.0.2"
	}
}
